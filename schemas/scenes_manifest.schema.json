{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$title": "Scene Manifest Schema",
  "$id": "https://example.com/schemas/scenes_manifest.schema.json",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "scenes"
  ],
  "properties": {
    "scenes": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "common.schema.json#/$defs/nonempty_string"
      }
    }
  }
}